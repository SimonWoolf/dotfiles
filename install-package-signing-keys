curl -fsSL https://packages.cloud.google.com/apt/doc/apt-key.gpg | sudo apt-key add -

curl -fsSL https://cli-assets.heroku.com/apt/release.key | sudo apt-key add -

curl -fsSL http://debian.datastax.com/debian/repo_key | sudo apt-key add -

curl -fsSL https://packages.erlang-solutions.com/ubuntu/erlang_solutions.asc | sudo apt-key add -

curl -fsSL https://github.com/rabbitmq/signing-keys/releases/download/2.0/rabbitmq-release-signing-key.asc | sudo apt-key add -

curl -fsSL https://packages.microsoft.com/keys/microsoft.asc | sudo apt-key add -

curl -fsSL https://download.teamviewer.com/download/linux/signature/TeamViewer2017.asc | sudo apt-key add -

sudo add-apt-repository ppa:twodopeshaggy/drive
